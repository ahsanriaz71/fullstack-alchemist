{
  // Application settings including reload time, email sending configuration, keywords, and labels
  "Application": {
    "ReloadTime": "05", // Time interval (in minutes) for reloading the news feeds
    "IsSendMessage": true, // Boolean flag to determine whether to send email notifications or not
    "MobileNo": "03001234567", // Phone number of the receiver for SMS notifications
    "ToEmail": "client@gmail.com", // Email address of the client to whom the notifications will be sent
    "EmailSubject": "News Feed Updates", // Subject of the email notifications
    "KeyWords": "Small,Things", // Keywords to be searched in the news feeds for relevant updates
    "Lebels": "Title" // Data labels from which information will be extracted, e.g., 'Title'

  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  // Email sending configuration including SMTP server details and sender's credentials
  "EmailConfiguration": {
    "Host": "smtp.gmail.com", // SMTP server host address
    "Port": 465, // Port number for SMTP server
    "Username": "Rss.Dummy@gmail.com", // Sender's Gmail account username
    "Password": "HashPassword123", // Hashed password for the sender's Gmail account
    "From": "Rss.Dummy@gmail.com", // Sender's email address
    "SSL": true // Boolean flag to enable SSL for secure email communication

  },

  "AllowedHosts": "*"
}
